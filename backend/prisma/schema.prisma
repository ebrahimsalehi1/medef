// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mysql"
  url      = "mysql://root:@localhost:3306/buildings"
}

generator client {
  provider = "prisma-client-js"
}

model Ebi_Users {
  user_id             Int      @id @default(autoincrement())
  user_login          String
  user_pass           String
  user_nicename       String
  user_email          String
  user_url            String
  user_registered     DateTime
  user_activation_key String
  user_status         Int
  display_name        String
}

model Ebi_UserMeta {
  umeta_id   Int    @id @default(autoincrement())
  user_id    Int
  meta_key   String
  meta_value String
}

model Ebi_Options {
  option_id    Int     @id @default(autoincrement())
  option_name  String
  option_value String
  autoload     Boolean
}

model Ebi_Country {
  country_id  Int         @id @default(autoincrement())
  country_name String
  Ebi_State   Ebi_State[]
}

model Ebi_State {
  state_id    Int         @id @default(autoincrement())
  state_name  String
  country_id  Int
  Ebi_Country Ebi_Country @relation(fields: [country_id], references: [country_id])
  Ebi_City    Ebi_City[]
}

model Ebi_City{
  city_id     Int         @id @default(autoincrement())
  city_name    String
  state_id    Int
  Ebi_State   Ebi_State   @relation(fields:[state_id],references:[state_id])
}

model Ebi_Lookup{
  lookup_id         String  @id
  lookup_title      String 
  Ebi_Lookup_Value  Ebi_Lookup_Value[]
}

model Ebi_Lookup_Value{
  lookup_value_id   Int           @id @default(autoincrement())
  lookup_key        String
  lookup_value      String
  lookup_id         String
  Ebi_Lookup        Ebi_Lookup    @relation(fields:[lookup_id],references:[lookup_id])
}


